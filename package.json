{
  "name": "dool",
  "version": "3.2.0",
  "description": "d-band tool & dev tool",
  "bin": {
    "dool": "bin/dool"
  },
  "scripts": {
    "lint": "eslint --ext .js src test/*.test.js",
    "build": "rimraf lib && babel src --out-dir lib",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test nyc mocha --no-timeouts",
    "report": "nyc report --reporter=html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "atpl-loader": "^0.2.0",
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.22.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.3.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.28.3",
    "dool-init": "^2.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "glob": "^7.1.2",
    "ip": "^1.1.5",
    "is-plain-object": "^2.0.3",
    "json-loader": "^0.5.4",
    "json5": "^1.0.0",
    "less": "^3.0.0",
    "less-loader": "^4.0.0",
    "map-json-webpack-plugin": "^1.1.0",
    "postcss-loader": "^2.0.0",
    "style-loader": "^0.20.0",
    "url-loader": "^0.6.1",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1",
    "worker-farm": "^1.4.1",
    "yargs": "^11.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-band/dool.git"
  },
  "keywords": [
    "d-band",
    "dev",
    "tool",
    "dool",
    "build",
    "server",
    "frontend"
  ],
  "author": "d-band",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/d-band/dool/issues"
  },
  "homepage": "https://github.com/d-band/dool#readme",
  "engines": {
    "node": ">= 4"
  },
  "devDependencies": {
    "babel-cli": "^6.22.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^4.0.1",
    "coveralls": "^3.0.0",
    "eslint": "^5.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.0.0",
    "nyc": "^11.0.2",
    "object-assign": "^4.1.0",
    "postcss-cssnext": "^3.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "webpack-md5-hash": "^0.0.6"
  }
}
